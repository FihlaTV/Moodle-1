<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aditi.ayush.nikhil.project1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/moodlepluslogo"
        android:name=".MyApp_cookie"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity

            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".MoodlePlus"
            android:label="@string/title_activity_moodle_plus"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


        <activity
            android:name=".CourseTab"
            android:label="@string/title_activity_course_tab"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name=".thread_detail"
            android:label="@string/title_activity_thread_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AssignDetails"
            android:label="@string/title_activity_assign_details"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
